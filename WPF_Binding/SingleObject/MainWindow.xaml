<Window x:Class="SingleObject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SingleObject"
        mc:Ignorable="d"
        Title="MainWindow" Height="165.686" Width="229.902">
    <StackPanel>
        <TextBlock Text="{Binding Name1}" FontSize="24"/>
        <TextBlock Text="{Binding Age}" FontSize="24"/>
        <Button Content="Change" FontSize="24" Click="OnChange"/>
    </StackPanel>
</Window>
